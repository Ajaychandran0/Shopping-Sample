<section>
{{#if user}}
  <div class="m-5 text-success">
    <h1>Welcome {{user.fName}}</h1>
  </div>
  {{/if}}

    <div class="row pt-5 ml-2">

      {{#each products}}
      <div class="col-md-3">
        <div class="card" style="width: 18rem;">
          <img class="card-img-top" style="width:5rem; height:7rem"
            src="data:image/jpeg;base64," 
            alt="Card image cap">
          <div class="card-body">
            <h5 class="card-title">{{this.Name}}</h5>
            <p class="card-text">{{this.Discription}}</p>
            <a href="#" class="btn btn-primary">Add to cart</a>
          </div>
        </div>
      </div>


      {{/each}}


     

    </div>
  </section>